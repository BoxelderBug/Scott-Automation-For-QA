-- ====================================================================
-- QUERY 1: DETAILED VIOLATIONS
-- ====================================================================
SELECT
    v.[location_key],
    v.[location_name],
    v.[service_order_key],
    v.[service_description],
    v.[work_dt],
    v.[technician_name],
    v.[quantity_used],
    v.[item_description],
    'Product not approved for this service' as violation_type
FROM [dbo].[vr_2067] v
INNER JOIN @ProductServiceMatrix m
    ON v.[item_description] = m.product_name
    AND v.[service_description] = m.service_name
    AND m.is_valid = 0
WHERE v.[work_dt] >= DATEADD(day, -7, CAST(GETDATE() AS date))
    AND v.[work_dt] < CAST(GETDATE() AS date)
ORDER BY v.[work_dt] DESC, v.[location_name];

-- ====================================================================
-- QUERY 2: ERROR PERCENTAGE BY TECHNICIAN
-- ====================================================================
SELECT
    tt.technician_name,
    ISNULL(te.error_count, 0) as error_count,
    tt.total_product_uses,
    CAST(ISNULL(te.error_count, 0) * 1.0 / NULLIF(tt.total_product_uses, 0) AS DECIMAL(6,5)) as error_percentage
FROM (
    SELECT
        technician_name,
        COUNT(*) as total_product_uses,
        SUM(CAST(quantity_used AS INT)) as total_quantity_used
    FROM [dbo].[vr_2067]
    WHERE work_dt >= DATEADD(day, -7, CAST(GETDATE() AS date))
        AND work_dt < CAST(GETDATE() AS date)
    GROUP BY technician_name
) tt
LEFT JOIN (
    SELECT
        v.technician_name,
        COUNT(*) as error_count
    FROM [dbo].[vr_2067] v
    INNER JOIN @ProductServiceMatrix m
        ON v.[item_description] = m.product_name
        AND v.[service_description] = m.service_name
        AND m.is_valid = 0
    WHERE v.work_dt >= DATEADD(day, -7, CAST(GETDATE() AS date))
        AND v.work_dt < CAST(GETDATE() AS date)
    GROUP BY v.technician_name
) te ON tt.technician_name = te.technician_name
ORDER BY error_percentage DESC;

-- ====================================================================
-- QUERY 3: OVERALL ERROR PERCENTAGE
-- ====================================================================
SELECT
    (SELECT COUNT(*) FROM [dbo].[vr_2067] v
     INNER JOIN @ProductServiceMatrix m ON v.[item_description] = m.product_name
         AND v.[service_description] = m.service_name AND m.is_valid = 0
     WHERE v.work_dt >= DATEADD(day, -7, CAST(GETDATE() AS date))
         AND v.work_dt < CAST(GETDATE() AS date)) as total_errors,

    (SELECT COUNT(*) FROM [dbo].[vr_2067]
     WHERE work_dt >= DATEADD(day, -7, CAST(GETDATE() AS date))
         AND work_dt < CAST(GETDATE() AS date)) as total_uses,

    CAST((SELECT COUNT(*) FROM [dbo].[vr_2067] v
          INNER JOIN @ProductServiceMatrix m ON v.[item_description] = m.product_name
              AND v.[service_description] = m.service_name AND m.is_valid = 0
          WHERE v.work_dt >= DATEADD(day, -7, CAST(GETDATE() AS date))
              AND v.work_dt < CAST(GETDATE() AS date)) * 100.0 /
         NULLIF((SELECT COUNT(*) FROM [dbo].[vr_2067]
                 WHERE work_dt >= DATEADD(day, -7, CAST(GETDATE() AS date))
                     AND work_dt < CAST(GETDATE() AS date)), 0) AS DECIMAL(5,2)) as overall_error_percentage;

-- ====================================================================
-- QUERY 4: QUANTITY OF ERRORS BY PRODUCT TYPE
-- ====================================================================
SELECT
    v.item_description as product_name,
    COUNT(*) as error_count
FROM [dbo].[vr_2067] v
INNER JOIN @ProductServiceMatrix m
    ON v.[item_description] = m.product_name
    AND v.[service_description] = m.service_name
    AND m.is_valid = 0
WHERE v.work_dt >= DATEADD(day, -7, CAST(GETDATE() AS date))
    AND v.work_dt < CAST(GETDATE() AS date)
GROUP BY v.item_description
ORDER BY error_count DESC;
